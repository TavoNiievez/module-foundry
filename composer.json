{
    "name": "tavoniievez/module-foundry",
    "description": "Foundry module for Codeception",
    "keywords": ["codeception", "foundry"],
    "homepage": "https://codeception.com/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Nieves"
        },
        {
            "name": "Jordan Wamser",
            "email": "jwamser@redpandacoding.com"
        }
    ],
    "minimum-stability": "RC",
    "require": {
        "php": "^8.1",
        "codeception/codeception": "^5.0",
        "zenstruck/foundry": "^1.36"
    },
    "require-dev": {
        "codeception/module-doctrine2": "^3.0",
        "codeception/module-symfony": "^3.1"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "classmap-authoritative": true
    }
}
